    "eslint-config-prettier": "^2.9.0",
    "eslint-plugin-import": "^2.13.0",
    "eslint-plugin-prettier": "^2.6.2",
    "firebase": "^4.11.0",
    "font-awesome": "^4.7.0",
    "history": "^4.7.2",
    "js-sha1": "^0.6.0",
    "lodash": "^4.17.11",
    "moment": "^2.22.2",
    "prettier": "^1.14.2",
    "query-string": "5.1.1",
    "react": "^16.4.1",
    "react-day-picker": "^7.2.4",
    "react-dom": "^16.4.1",
    "react-google-maps": "^9.4.5",
